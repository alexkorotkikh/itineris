{
  "name": "router",
  "version": "0.1.0",
  "description": "",
  "main": "dist/collector.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "build:watch": "tsc-watch server.ts --outDir ./dist --onSuccess 'node ./dist/server.ts'",
    "start": "nodemon ./dist/collector.js",
    "pretest": "npm run build",
    "lint": "tslint --project .",
    "test": "mocha --require dist/test/etcd-daemon.js dist/test/*-test.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run build || echo 'In install cycle, skipping prepublish'"
  },
  "repository": {
    "type": "git",
    "url": "mabels/sla-clock"
  },
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/mabels/sla-clock/issues"
  },
  "homepage": "https://github.com/mabels/sla-clock#readme",
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/mocha": ">=2.2.33",
    "chai": "^4.1.1",
    "mocha": ">=2.5.3",
    "rimraf": "^2.6.1",
    "tsc-watch": "^1.0.7",
    "tslint": "^5.6.0",
    "typescript": ">=2.1.5"
  },
  "dependencies": {
    "@types/classnames": ">=0.0.32",
    "@types/fs-extra": ">=3.0.0",
    "@types/uuid": "^3.4.0",
    "@types/request-promise": "4.1.37",
    "@types/winston": ">=0.0.32",
    "@types/yargs": "^8.0.2",
    "ipaddress": "^0.2.5",
    "promise-etcd": ">=2.1.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "rxjs": "^5.4.3",
    "uuid": ">=3.1.0",
    "winston": ">=2.2.0",
    "yargs": "^9.0.1"
  },
  "publishConfig": {
    "tag": "next"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
